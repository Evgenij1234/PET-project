@{
    ViewBag.Title = "Страница книги";
}
@model GeneralModel
@await Html.PartialAsync("Header")
<script src="~/js/jqery.js"></script>
<main class="main_book_page">
    @foreach (var book in Model.Books)
    {
        if (book.ID == Convert.ToInt32(ViewData["Message"]))
        {
            <div class="main_book_page_start">
                <div class="book_page-start-label">
                    <p class="book_page-label">
                        @book.NameBook
                    </p>
                    <p class="book_page-label1">
                        @book.AutorBook
                    </p>
                </div>
                <div class="book_page_start-content">
                    <div class="book_page_start-content-box-img">
                        <img class="ook_page_start-content-img" src="~/@book.ImgBigBook" alt="">
                    </div>
                    <div class="book_page_start-content-box-assaid">
                        <div class="box-assaid_box">
                            <div class="box-assaid_box1">
                                <div class="assaid_box1-text">Цена</div>
                                <div class="assaid_box1-int">@book.PraiseBook р.</div>
                            </div>
                            <div class="box-assaid_box1">
                                <script type="text/jscript">
                                    function CreateBook_pageAddFavorites() {
                                        $.get("/Books/Book_pageAddFavorites/@book.ID");
                                    };
                                </script>
                                <a href="" onclick="CreateBook_pageAddFavorites()" class="box-assaid_box1-a">
                                    <div class="assaid_box1-text">
                                        В избранное
                                    </div>
                                    <div class="assaid_box1-svg">
                                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
                                        width="30px" height="30px">
                                            <image x="0px" y="0px" width="30px" height="30px"
                                            xlink:href="data:img/png;base64," />
                                        </svg>
                                    </div>
                                </a>
                            </div>









                            <div class="box-assaid_box1">
                                <script type="text/jscript">
                                    function CreateBook_pageAddCart() {
                                        $.get("/Books/CreateBook_pageAddCart/@book.ID");
                                    };
                                </script>
                                <a href="" onclick="CreateBook_pageAddCart()" class="box-assaid_box1-a">
                                    <div class="assaid_box1-text">
                                        В корзину
                                    </div>
                                    <div class="assaid_box1-svg">
                                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
                                        width="30px" height="30px">
                                            <image x="0px" y="0px" width="30px" height="30px"
                                            xlink:href="data:img/png;base64," />
                                        </svg>
                                    </div>
                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="main_book_page_centr">
                <p class="book_page-label">
                    Анотация
                </p>
                <p class="book_page_centr-text">
                    @book.AnatationBook
                </p>
            </div>
            <div class="main_book_page_end">
                <p class="book_page-label">
                    Характеристики
                </p>
                <div class="book_page-box">
                    <ul class="book_page-box_spisok">
                        <li class="book_page-box_spisok-li">
                            <div class="spisok-li_start">
                                Категория...........................
                            </div>
                            <div class="spisok-li_end">
                                @book.CategoreBook
                            </div>
                        </li>
                        <li class="book_page-box_spisok-li">
                            <div class="spisok-li_start">
                                Автор...................................
                            </div>
                            <div class="spisok-li_end">
                                @book.AutorBook
                            </div>
                        </li>
                        <li class="book_page-box_spisok-li">
                            <div class="spisok-li_start">
                                Год издания.........................
                            </div>
                            <div class="spisok-li_end">
                                @book.AgeBook
                            </div>
                        </li>
                        <li class="book_page-box_spisok-li">
                            <div class="spisok-li_start">
                                Переплёт..............................
                            </div>
                            @{if (@book.BindingBook == false)
                            {
                                <div class="spisok-li_end">
                                    Твердый
                                </div>
                            }
                            else
                            {
                               <div class="spisok-li_end">
                                   Мягкий
                               </div>
                            }
                            }
                           
                        </li>
                    </ul>
                </div>
            </div>
        }
    }
        </main>
@await Html.PartialAsync("Footer")



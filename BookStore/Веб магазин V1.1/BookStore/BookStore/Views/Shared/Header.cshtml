@model GeneralModel
    <Header class="header">

        <!--Верхняя часть хедера-->

        <div class="header_conteiner-top">
            <!--контейнер верхних элементов шапки сайта-->
            <div class="header_slogan">
                <!--элемент слоган-->
                <span class="slogan-Semenova">Официаьный сайт Семенова Е.А</span>
            </div>
            <div class="header_location" id="location2">
                <!--контейнер местоположения-->
                <!--Кнопка местоположения-->
                <button class="location__link-form-button" onmousedown="viewFormLocation()">
                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="20px"
                         height="34px">
                        <image x="0px" y="5px" width="14px" height="20px"
                               xlink:href="data:img/png;base64," />
                    </svg><!--картинка местоположения-->
                Местоположение: @ViewData["locality"]
                <!--надпись местоположения-->


                </button>
            </div>
            <div></div>
            <div class="header_contacts">
                <!--контейнер контактов-->
                <span>Контакты: +79233294361</span>
            </div>
            <div class="header_entrance">
                <!--контейнер авторизация вход-->
                <ol class="entrance_list">
                    <li class="entrance_list__vhod">
                        <!--кнопка Вход-->
                        <button class="ist__vhod-button" onmousedown="viewFormVhod()" id="Vhod2">
                            Вход
                        </button>
                    </li>
                    <li><span>/</span></li>
                    <li class="entrance_list__autorisacia">
                        <!--кнопка Авторизация-->
                        <button class="ist__autorisacia-button" onmousedown="viewFormReg()" id="reg2">
                            Регистрация
                        </button>
                    </li>
                </ol>
            </div>
        </div>

        <!--конец верхней части хедера-->
        <!--Средняя часть хедера-->

        <div class="header_conteiner-center">
            <!--Средняя часть хедера-->
            <div class="conteiner-center_logo">
                <!--контейнер логотипа-->
                <a href="/" class="logo_svg">
                    <!--контейнер картинки логотипа-->
                    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="318px"
                         height="45px">
                        <text kerning="auto" font-family="Aria" fill="rgb(0, 0, 0)" font-size="63.5px" x="0px"
                              y="44.19px">
                            <tspan font-size="30px" fill="#1F2022">HARDBOOK</tspan>
                        </text>
                    </svg><!--картинка логотипа-->
                </a>
            </div>

            <!--поисковая строка-->

            <div class="conteiner-center_search">
                <!--контейнер поисковой строки-->
                <div class="form_content">
                    <!--форма поисковой строки-->
                <form action="/Search/Search" method="get" autocomplete="off" class="search_form">
                        <!--инпут ввода книг-->
                        <input id="#" type="text" placeholder="Поиск книг" name="name" autocomplete="off"
                               class="large_pole_input">
                        <button type="submit" class="button_search">
                            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
                                 width="25px" height="25px">
                                <image opacity="0.502" x="0px" y="0px" width="25px" height="25px"
                                       xlink:href="data:img/png;base64," />
                            </svg>
                        </button>
                    </form>
                </div>
            </div>
            <!--Избранное и корзина-->
            <div class="conteiner-center_favorites_and_basket">
                <div class="favorites_box">






                    <!--сылка на Избранное-->
                <a href="/Favorites/Favorites" method="post" class="avorites_box_href">
                        <div class="avorites_svg">
                            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
                                 width="25px" height="25px">
                                <image x="0px" y="0px" width="25px" height="25px"
                                       xlink:href="data:img/png;base64," />
                            </svg>
                        </div>
                        <span>Избранное</span>
                    </a>








                </div>
                <div class="conteiner-center_basket">

                    <!--сылка на Корзину-->
                <a href="/Cart/Cart" class="center_basket_href">
                        <div class="basket_svg">
                            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
                                 width="25px" height="25px">
                                <image x="0px" y="0px" width="25px" height="25px"
                                       xlink:href="data:img/png;base64," />
                            </svg>
                        </div>
                        <span>Корзина</span>
                    </a>
                </div>
            </div>
        </div>

        <!--Конец средней части хедера-->
        <!--Нижняя часть хедера-->

        <div class="header_conteiner-end">
            <!--бох нижней части хедера-->
            <div class="conteiner-end_catalog">
                <!--бох каталога-->
                <div class="catalog_box">
                    <!--бох каталога(вложеный)-->
                    <button class="catalog_box_button">
                        Каталог <div class="catalog_box_button_3">≡</div>
                    </button><!--кнопка каталога-->
                    <div class="catalog_box_content">
                        <!--бох скрытого каталога-->
                        <!--ссылка на боевики-->
                    <a href="/Search/Search/boeviki/?boeviki=on&autor=&age=" class="catalog_box_content_a">
                            Боевики
                        </a>
                        <!--ссылка на детективы-->
                    <a href="/Search/Search/boeviki/?detektiv=on&autor=&age=" class="catalog_box_content_a">
                            Детективы
                        </a>
                        <!--ссылка на Любовные романы-->
                    <a href="/Search/Search/boeviki/?love=on&autor=&age=" class="catalog_box_content_a">
                            Любовные романы
                        </a>
                        <!--ссылка на Приключения-->
                    <a href="/Search/Search/boeviki/?prikluch=on&autor=&age=" class="catalog_box_content_a">
                            Приключения
                        </a>
                        <!--ссылка на Фантастика-->
                    <a href="/Search/Search/boeviki/?fantastika=on&autor=&age=" class="catalog_box_content_a">
                            Фантастика
                        </a>
                        <!--ссылка на Фентези-->
                    <a href="/Search/Search/boeviki/?fentezi=on&autor=&age=" class="catalog_box_content_a">
                            Фентези
                        </a>
                    </div>
                </div>
            </div>

            <!--бокс новинки, авторы, доставк и тд-->

        <div class="conteiner-end_div" >
                <!--сылка на Новинки-->
            <a href="/" class="conteiner-end_div_a">
                <span class="conteiner-end_div_a_span">Новинки</span>
            </a>
            </div>
            <div class="conteiner-end_div">
                <!--сылка на Авторы-->
                <a href="/" class="conteiner-end_div_a">
                    <span class="conteiner-end_div_a_span">Авторы</span>
                </a>
            </div>
            <div class="conteiner-end_div">
                <!--сылка на доставка-->
            <a href="/Design/Design" class="conteiner-end_div_a">
                    <span class="conteiner-end_div_a_span">Доставка</span>
                </a>
            </div>
            <div class="conteiner-end_div">
                <!--сылка на О нас-->
            <a href="/Design/about_Us" class="conteiner-end_div_a">
                    <span class="conteiner-end_div_a_span">О нас</span>
                </a>
            </div>
            <div class="conteiner-end_div"> <a href="#"></a></div>
            <div class="conteiner-end_div">
                <!--сылка на Личный кабинет-->
            <a href="#" method="get" class="conteiner-end_div_a" onmousedown="viewFormVhod()" id="Vhod2">
                    <span class="conteiner-end_div_a_span">Личный кабинет</span>
                </a>
            </div>
        </div>

        <!-- скрытая форма вход-->

        <div class="header_form-box" id="Vhod1">
            <div class="header_form_vhod">
                <div class="form_vhod_exit-box">
                    <!-- кнопка закрыть форму вход-->
                    <button class="form_vhod_exit" onmousedown="viewFormVhodExit()">
                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="20px"
                             height="20px">
                            <image x="0px" y="0px" width="20px" height="20px"
                                   xlink:href="data:img/png;base64," />
                        </svg>
                    </button>
                </div>
                <div class="form_vhod-text">
                    Вход
                </div>
                <div>
                    <hr width="200" size="1" color="#000000" />
                </div>
                <div class="form_vhod-form-box">
                    <!-- форма входа-->
                <form class="form_vhod-form" action="/Users/Personal_area1" method="get">
                        <!-- инпут емайла-->
                    <input class="form_vhod-form-input" type="text" name="EmailUsers" placeholder="Емайл...">
                        <!-- инпут пороля-->
                    <input class="form_vhod-form-input" type="text" name="PaswordlUsers" placeholder="Пароль...">
                        <!-- кнопка войти входа-->
                        <button class="form_vhod-form-button">
                            Войти
                        </button>
                    </form>
                </div>
            </div>
        </div>

        <!--  скрытая форма регистрация-->

        <div class="header_reg-box" id="reg1">
            <div class="header_reg">
                <div class="form_reg_exit-box">
                    <!-- кнопка закрыть форму регистрация-->
                    <button class="form_reg_exit" onmousedown="viewFormRegExit()">
                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="20px"
                             height="20px">
                            <image x="0px" y="0px" width="20px" height="20px"
                                   xlink:href="data:img/png;base64," />
                        </svg>
                    </button>
                </div>
                <div class="form_reg-text">
                    Регистрация
                </div>
                <div>
                    <hr width="200" size="1" color="#000000" />
                </div>
                <div class="form_reg-form-box">
                    <!-- форма регистрации-->
                <form class="form_reg-form" action="/Users/Personal_area2" method="post">
                    <!-- инпут ID, не виден пользователю-->
                    <input type="hidden" name="ID" value="item.CustomerId" />
                        <!-- инпут емайла-->
                    <input class="form_reg-form-input" type="text" name="EmailUsers" placeholder="Емайл...">
                        <!-- инпут пороля-->
                    <input class="form_reg-form-input" type="text" name="PaswordlUsers" placeholder="Пароль...">
                        <!-- инпут имя-->
                    <input class="form_reg-form-input" type="text" name="NameUsers" placeholder="Имя">
                        <!-- инпут фамилия-->
                    <input class="form_reg-form-input" type="text" name="FemileUsers" placeholder="Фамилия">


                        <!-- чекбокс обработка перс данных-->
                        <input class="form_reg-form-checkbox" type="checkbox" name="agree_to_data" id="reg1">
                        <span class="form_reg-form-checkbox-text">Согласен на обработку <br> персональных данных</span>

                        <!-- чекбокс 18 лет-->
                        <input class="form_reg-form-checkbox" type="checkbox" name="coming_of_age" id="reg2">
                        <span class="form_reg-form-checkbox-text">
                            Подтверждаю, что мне есть 18 лет <br> и я не мамкин
                            сосунок!
                        </span>
                        <!-- кнопка сохранить регистрации-->
                    <input type="submit" class="form_reg-form-button" value="Сохранить">
                    </form>
                </div>
            </div>
        </div>

        <!-- местоположение-->

        <div class="header_location1" id="location1">
            <div class="header_location_form">
                <div class="location_form_exit-box">
                    <!-- кнопка закрыть форму местоположение-->
                    <button class="form_location_exit" onmousedown="viewFormLocationExit()">
                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="20px"
                             height="20px">
                            <image x="0px" y="0px" width="20px" height="20px"
                                   xlink:href="data:img/png;base64," />
                        </svg>
                    </button>
                </div>
                <div class="form_location-text">
                    Местоположение
                </div>
                <div>
                    <hr width="200" size="1" color="#000000" />
                </div>
                <div class="form_location-form-box">
                    <!-- форма местоположение-->

                <form class="form_location-form" action="/Home/Main_page" method="post">
                        <!-- инпут местоположение-->
                        <input class="form_location-form-input" type="text" name="location1" placeholder="Город...">
                        <!-- кнопка записи местоположение-->
                        <button class="form_location-form-button">
                            Записать
                        </button>
                    </form>


                </div>
            </div>
        </div>
    </Header>

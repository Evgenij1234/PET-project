@model GeneralModel
@{
    ViewBag.Title = "Личный кабинет";
}
@await Html.PartialAsync("Header")
<!--Главная часть-->
<main class="main_personal_area">
    <div class="main_personal_area-text">
        Личный кабинет
    </div>
    <div class="main_personal_area-content">
        <div>
            <aside class="main_personal_area_aside">
                <div class="personal_area_aside-box">
                    <div class="personal_area_aside-box-svg">
                        <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink"
                             width="65px" height="90px">
                            <image x="0px" y="0px" width="65px" height="90px"
                                   xlink:href="data:img/png;base64," />
                        </svg>
                    </div>
                    <div class="personal_area_aside-box-exit">
                        <a href="Delete\@ViewData["ID"]" name="id" class="personal_area_aside-a">
                            Удалить
                        </a>
                    </div>
                </div>
            </aside>
        </div>

        <!--Центральная часть с формами-->

        <div class="main_personal_area_centr">
            <div class="main_personal_area_centr-FIO">
                <form name="form1" class="personal_area_centr-FIO-form" action="/Users/Update" method="post">
                    <p class="main_personal_area_centr-pasvord-text">Личные данные</p>
                    <label for="" class="ersonal_area_centr-FIO-data">Имя</label>
                    <input name="NameUsers" type="text" class="_area_centr-FIO-form-input" value="@ViewData["NameUsers"]" placeholder="Имя...">
                    <label for="" class="ersonal_area_centr-FIO-data">Фамилия</label>
                    <input name="FemileUsers" type="text" class="_area_centr-FIO-form-input" value="@ViewData["FemileUsers"]" placeholder="Фамилия">
                    <label for="" class="ersonal_area_centr-FIO-data">Отчество</label>
                    <input name="SurnameUsers" type="text" class="_area_centr-FIO-form-input" value="@ViewData["SurnameUsers"]" placeholder="Отчество">
                    <label for="" class="ersonal_area_centr-FIO-data">Email</label>
                    <input name="EmailUsers" type="text" class="_area_centr-FIO-form-input" value="@ViewData["EmailUsers"]" placeholder="Емайл...">
                    <label for="" class="ersonal_area_centr-FIO-data">Дата рождения</label>
                    <input name="DateUsers" type="date" class="_area_centr-FIO-form-input" value="@ViewData["DateUsers"]" placeholder="01.01.1991">
                    <label for="" class="ersonal_area_centr-FIO-data">Пол</label>
                    <div class="area_centr-FIO-form-checkbox_box">
                        <input type="radio" name="GenderUsers" class="_area_centr-FIO-form-checkbox" value="@ViewData["GenderUsers"]">
                        <div>Мужской</div>
                    </div>
                    <div class="area_centr-FIO-form-checkbox_box">
                        <input type="radio" name="GenderUsers" class="_area_centr-FIO-form-checkbox" value="@ViewData["GenderUsers"]">
                        <div>Женский</div>
                    </div>
                    <p class="main_personal_area_centr-pasvord-text">Смена пароля</p>
                    <label for="" class="ersonal_area_centr-FIO-data">Новый пароль</label>
                    <input name="PaswordlUsers" type="text" class="_area_centr-FIO-form-input" value="@ViewData["PaswordlUsers"]">
                    <input name="ID" type="hidden" class="_area_centr-FIO-form-input" value="@ViewData["ID"]">
                    <input type="submit" class="_area_centr-FIO-form-button" value="Сохранить">
                </form>
            </div>
        </div>
    </div>


</main>

@await Html.PartialAsync("Footer")
